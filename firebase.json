{
  "hosting": {
    "public": "web-build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/__/firebase/**",
        "destination": "/__/firebase/init.js"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
